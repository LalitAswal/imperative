{
    "$schema": "./ProfInfoApp.schema.json",
    "profiles": {
        "base_glob": {
            "type": "base",
            "properties": {
                "user": "userNameBase",
                "password": "passwordBase",
                "rejectUnauthorized": false
            }
        },
        "LPAR1": {
            "type": "zosmf",
            "properties": {
                "host": "LPAR1.your.domain.net",
                "port": 1234,
                "responseFormatHeader": true
            },
            "profiles": {
                "tsoProfName": {
                    "type": "tso",
                    "properties": {
                        "account": "12345678",
                        "characterSet": "697",
                        "codePage": "1047",
                        "columns": 80,
                        "logonProcedure": "LOGPROC",
                        "regionSize": 4096,
                        "rows": 24
                    },
                    "profiles": {
                        "tsoSubProfName": {
                            "type": "tso",
                            "properties": {
                                "account": "IZUACCT"
                            }
                        }
                    }
                }
            }
        },
        "LPAR2": {
            "type": "zosmf",
            "properties": {
                "host": "LPAR1.your.domain.net",
                "port": 6789,
                "responseFormatHeader": true
            }
        },
        "LPAR3": {
            "type": "zosmf",
            "properties": {
                "host": "LPAR1.your.domain.net",
                "port": 456
            }
        }
    },
    "defaults": {
        "zosmf": "LPAR1",
        "tso": "LPAR1.tsoProfName",
        "base": "base_glob"
    },
    "plugins": [],
    "secure": []
}